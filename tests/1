import { test, expect } from '@playwright/test';

test('test', async ({ page }) => {
  await page.goto('https://beta-app.sigparser.com/');
  await page.getByRole('button', { name: 'Log in with Email' }).click();
  await page.getByPlaceholder('Email').click();
  await page.getByPlaceholder('Email').click();
  await page.getByPlaceholder('Email').fill('test+stafona+testuser1@dragnettech.com');
  await page.getByPlaceholder('Password').click({
    modifiers: ['ControlOrMeta']
  });
  await page.getByPlaceholder('Password').fill('test+stafona+testuser1@dragnettech.com');
  await page.getByRole('img', { name: 'Show' }).click();
  await page.getByPlaceholder('Password').click({
    clickCount: 3
  });
  await page.getByPlaceholder('Password').fill('');
  await page.getByPlaceholder('Password').click();
  await page.getByPlaceholder('Password').fill('Panda@2024');
  await page.getByRole('button', { name: 'Log In' }).click();
  await page.goto('https://beta-app.sigparser.com/Account/App/#/Dashboard');
  await page.getByRole('button', { name: 'Search' }).click();
  await page.getByRole('cell', { name: 'Email Address', exact: true }).getByPlaceholder('Search...').click();
  await page.getByRole('cell', { name: 'Email Address', exact: true }).getByPlaceholder('Search...').fill('raj');
  await page.getByRole('row', { name: 'Raj Gupta raj.gupta@vcfparser' }).getByLabel('').check();
  await page.getByRole('row', { name: 'Raj Gupta raj@vcfparser.com' }).getByLabel('').check();
  await page.getByRole('button', { name: 'Update' }).click();
  await page.locator('section').filter({ hasText: /^SelectMerge RecordsMerge the selected records together\.$/ }).getByRole('button').click();
  await page.getByRole('row', { name: 'Raj Gupta raj@vcfparser.com vcfparser 1 | 3 | 0 Aug 4, 2024 Nov 7, 2023 ' }).getByRole('radio').check();
  await page.getByRole('button', { name: 'Save' }).click();
  await page.getByRole('button', { name: '' }).click();
  await page.getByRole('link', { name: 'Contacts', exact: true }).click();
  await page.getByRole('button', { name: 'Search' }).click();
  await page.getByRole('cell', { name: 'Email Address' }).getByPlaceholder('Search...').fill('raj.gupta@vcfparser.com');
  await page.getByRole('cell', { name: 'Email Address' }).getByPlaceholder('Search...').click();
  await expect(page.getByRole('button', { name: '' })).toBeVisible();
  await page.getByRole('button', { name: '' }).click();
  await page.getByRole('cell', { name: 'Email Address' }).getByPlaceholder('Search...').fill('raj@vcfparser.com');
  await page.getByRole('cell', { name: ' Email Address' }).getByPlaceholder('Search...').click();
  await page.getByRole('cell', { name: 'Email Address raj@vcfparser.' }).getByPlaceholder('Search...').click();
  await page.getByRole('cell', { name: 'Email Address raj@vcfparser.' }).getByPlaceholder('Search...').click();
  await page.getByRole('cell', { name: 'Email Address raj@vcfparser.' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address raj@vcfparser.' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address raj@vcfparser.' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address raj@vcfparser.' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address raj@vcfparser.' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address raj@vcfparser.' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address raj@vcfparser.' }).getByPlaceholder('Search...').fill('');
  await page.getByRole('button', { name: 'Search' }).click();
  await page.getByRole('cell', { name: 'Email Address', exact: true }).getByPlaceholder('Search...').fill('raj@vcfparser.com ');
  await page.getByRole('cell', { name: 'Email Address', exact: true }).getByPlaceholder('Search...').click();
  await page.getByRole('cell', { name: 'Email Address raj@vcfparser.' }).getByPlaceholder('Search...').fill('raj@vcfparser.com');
  await page.getByText('Raj Gupta').click();
  await page.locator('#modal i').nth(2).click();
  await page.locator('#modal i').nth(2).click({
    button: 'right'
  });
  await page.locator('#modal i').nth(2).click();
  await expect(page.locator('#modal i').nth(2)).toBeVisible();
  await page.getByText('Details').click();
  await expect(page.locator('#modal').getByRole('cell', { name: 'raj@vcfparser.com' })).toBeVisible();
  await page.getByRole('cell', { name: 'raj.gupta@vcfparser.com', exact: true }).click();
  await expect(page.getByRole('cell', { name: 'raj.gupta@vcfparser.com', exact: true })).toBeVisible();
  await expect(page.getByRole('row', { name: 'raj@vcfparser.com 1 | 3 | 0' }).locator('img')).toBeVisible();
  await page.getByRole('heading', { name: 'Email Addresses  ' }).locator('i').nth(1).click();
  await page.getByPlaceholder('Search', { exact: true }).click();
  await page.getByPlaceholder('Search', { exact: true }).fill('elon');
  await page.locator('#merged-records-search').getByRole('button', { name: 'Search' }).click();
  await page.getByPlaceholder('Search', { exact: true }).click();
  await page.getByPlaceholder('Search', { exact: true }).fill('mohamed');
  await page.locator('#merged-records-search').getByRole('button', { name: 'Search' }).click();
  await page.getByRole('row', { name: 'Mohamed El-Masri mohamed.el-' }).getByRole('checkbox').check();
  await page.getByRole('row', { name: 'Mohamed El-Masri mohamed@' }).getByRole('checkbox').check();
  await page.getByRole('row', { name: 'Mohamed El-Masri mohamed.elmasri@vcfparser.com vcfparser 0 | 0 | 1 Nov 10, 2023 Nov 9,' }).getByRole('checkbox').check();
  await page.getByRole('row', { name: 'Mohamed El-Masri mohamed.el-' }).getByRole('checkbox').uncheck();
  await page.getByRole('row', { name: 'Mohamed El-Masri mohamed@' }).getByRole('checkbox').uncheck();
  await page.getByRole('row', { name: 'Mohamed El-Masri mohamed.elmasri@vcfparser.com vcfparser 0 | 0 | 1 Nov 10, 2023 Nov 9,' }).getByRole('checkbox').uncheck();
  await page.getByRole('row', { name: 'Mohamed El-Masri mohamed.el-' }).getByRole('checkbox').check();
  await page.getByRole('button', { name: 'Add Selected' }).click();
  await page.getByRole('button', { name: 'Save' }).click();
  await page.getByRole('button', { name: '' }).click();
  await page.getByRole('button', { name: 'Search' }).click();
  await page.getByRole('button', { name: 'Search' }).click();
  await page.getByRole('cell', { name: 'Email Address', exact: true }).getByPlaceholder('Search...').click();
  await page.getByRole('cell', { name: 'Email Address', exact: true }).getByPlaceholder('Search...').fill('mohamed.el-masri@vcfparser.com');
  await page.getByRole('cell', { name: 'Email Address mohamed.el-' }).getByPlaceholder('Search...').click();
  await page.getByRole('cell', { name: 'Email Address mohamed.el-' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address mohamed.el-' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address mohamed.el-' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address mohamed.el-' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address mohamed.el-' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address mohamed.el-' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address mohamed.el-' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address mohamed.el-' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address mohamed.el-' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address mohamed.el-' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address mohamed.el-' }).getByPlaceholder('Search...').press('ArrowRight');
  await page.getByRole('cell', { name: 'Email Address mohamed.el-' }).getByPlaceholder('Search...').fill('mohamed');
  await page.getByRole('cell', { name: 'Mohamed El-Masri ' }).locator('a').click();
  await page.getByText('Details').click();
  await page.getByRole('button', { name: '' }).click();
  await page.getByRole('cell', { name: 'Mohamed El-Masri ' }).locator('a').click();
  await page.getByText('Details').click();
  await page.getByRole('button', { name: '' }).click();
  await page.getByRole('cell', { name: 'Email Address mohamed ' }).getByPlaceholder('Search...').click();
  await page.getByRole('button', { name: '' }).click();
});